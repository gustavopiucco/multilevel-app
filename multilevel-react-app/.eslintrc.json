{
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "plugins": [
        "react"
    ],
    "rules": {
        "no-console": [
            "error",
            {
                "allow": [
                    "log",
                    "error"
                ]
            }
        ],
        "no-unused-vars": [
            "error",
            {
                "args": "none"
            }
        ],
        "react/prop-types": [
            0
        ],
        "react/no-find-dom-node": [
            0
        ]
    },
    "env": {
        "commonjs": true,
        "browser": true,
        "es6": true,
        "node": true
    }
}